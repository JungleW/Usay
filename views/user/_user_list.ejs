<ul class="list-group" id="user_list">
    <% for(let i=0, len=list.length; i<len; i++){ var follower = list[i].to; if(!follower) continue;%><!-- for1 -->
     <li class="list-group-item post-item">
        <h1 class="author">
            <a href="/user/user_view?userId=<%= follower._id %>"><img src="<%= follower.avatar %>"></a>
            <% if(follower.nickname){ %> <%= follower.nickname %>(<%= follower.username %>) <% }else{%><%= follower.username %><% } %>
            <a href="javascript:void(0)" class="delFollow flr a-active link-no-decoration glyphicon glyphicon-trash" data-userid="<%= follower._id %>"></a>
            <small><% if(follower.bio){ %><%= follower.bio %><% }else{ %>个性签名...<% } %></small>
        </h1>
    </li>
    <% } %>
		</ul>
 
<div class="page">
    <ul class="pagination" id="m_pag">

    </ul>
</div>

